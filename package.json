{
  "name": "electron-olimpiadas-wissen",
  "version": "1.1.0",
  "description": "AplicaciÃ³n para trabajar las olimpiadas sin necesidad de internet ni engorrosas instalaciones",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder -p always",
    "publish": "electron-builder --mac --win -p always",
    "postinstall": "electron-builder install-app-deps"
  },
  "repository": "https://github.com/bluesky777/electron-olimpiadas-wissen.git",
  "keywords": [
    "Electron",
    "Olimpiadas",
    "wissen"
  ],
  "author": "Joseth Guerrero",
  "license": "CC0-1.0",
  "build": {
    "appId": "olimpiadasWissen",
    "mac": {
      "category": "com.education.olimpiadas.wissen",
      "icon": "build/icons/icon.icns",
      "target":"pkg"
    },
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    }
  },
  "dependencies": {
    "axios": ">=0.21.2",
    "base64-img": "^1.0.4",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.4",
    "dotenv": "^5.0.1",
    "electron-is-dev": "^0.3.0",
    "electron-log": "^2.2.16",
    "electron-updater": "^4.0.0",
    "express": "^4.15.2",
    "express-fileupload": "^0.4.0",
    "ip": "^1.1.5",
    "jsonwebtoken": "^8.3.0",
    "node-excel-export": "^1.4.4",
    "socket.io": "^2.1.1"
  },
  "devDependencies": {
    "@angular/cli": "^6.0.5",
    "electron": "^18.2.4",
    "electron-builder": "23.0.9",
    "electron-reload": "^1.2.5"
  }
}
